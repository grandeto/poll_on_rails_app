<p id="notice"><%= notice %></p>

<h1>Poll Options</h1>

<table>
  <thead>
    <tr>
      <th>Poll</th>
      <th>Poll</th>
      <th>Option</th>
      <th>Priority</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @poll_options.each do |poll_option| %>
      <tr>
        <td><%= poll_option.poll_id %></td>
        <td><%= poll_option.poll_id %></td>
        <td><%= poll_option.option %></td>
        <td><%= poll_option.priority %></td>
        <td><%= link_to 'Show', poll_option %></td>
        <td><%= link_to 'Edit', edit_poll_option_path(poll_option) %></td>
        <td><%= link_to 'Destroy', poll_option, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Poll Option', new_poll_option_path %>
